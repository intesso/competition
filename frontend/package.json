{
  "name": "competition-frontend",
  "description": "Frontend App for the competition system",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "start": "cd dist && live-server --entry-file=index.html --port=7777",
    "pm2": "pm2 start npm --name controller-web -- run start",
    "pm2-localtunnel": "pm2 start ./localtunnel.sh --name localtunnel",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.79",
    "@mui/material": "^5.6.3",
    "@mui/x-data-grid": "^5.17.10",
    "@mui/x-date-pickers": "^5.0.3",
    "axios": "^0.27.2",
    "fscreen": "^1.2.0",
    "koa-send": "^5.0.1",
    "lodash-es": "^4.17.21",
    "luxon": "^2.5.0",
    "notistack": "^2.0.8",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-joystick-component": "^4.0.0",
    "react-router-dom": "^6.3.0",
    "swr": "^1.3.0",
    "usehooks-ts": "^2.5.2"
